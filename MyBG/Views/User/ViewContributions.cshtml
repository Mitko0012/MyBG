@model UserContribsModel

<h1>Contributions made by @Model.UserName</h1>
@{
	
	foreach (EditModel model1 in Model.Edits)
	{
		<div class="editDiv">
			<h3>Edit #@model1.PageIndex for @model1.PageToEdit.Title</h3>
			<a class="edit-redirect" asp-controller="Edit" asp-action="ViewEdit" asp-route-pageId="@model1.PageToEdit.Id" asp-route-editIndex="@model1.PageIndex">View edit</a>
		</div>
	}
}
